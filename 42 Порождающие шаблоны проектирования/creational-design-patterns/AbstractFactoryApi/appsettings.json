{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "NotificationSettings": {
    "Email": {
      "SmtpServer": "smtp.example.com",
      "Port": 587,
      "Username": "your-username",
      "Password": "your-password"
